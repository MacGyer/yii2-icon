{
    "name": "macgyer/yii2-icon",
    "minimum-stability": "stable",
    "description": "Icon helper classes and widgets for Yii2",
    "keywords": [
        "icon",
        "yii2",
        "widgets",
        "fontawesome",
        "bootstrap",
        "material icons"
    ],
    "homepage": "http://github.com/macgyer/yii2-icon",
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Christoph Erdmann",
            "homepage": "http://www.matero.de",
            "role": "Developer"
        }
    ],
    "version": "1.0.0",
    "require": {
        "php": ">=5.6.0",
        "yiisoft/yii2": "~2.0.0"
    },
    "autoload": {
        "psr-4": {
            "macgyer\\yii2icon\\": "src"
        }
    },
    "archive": {
        "exclude": ["/bin"]
    }
}
